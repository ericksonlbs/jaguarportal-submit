{
  "openapi": "3.0.1",
  "info": {
    "title": "WebJaguarPortal",
    "version": "1.0"
  },
  "paths": {
    "/api/Analyzes": {
      "post": {
        "tags": [
          "Analyzes"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AnalysisControlFlowModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AnalysisControlFlowModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AnalysisControlFlowModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/AnalysisControlFlowResponseModel"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnalysisControlFlowResponseModel"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/AnalysisControlFlowResponseModel"
                }
              }
            }
          }
        }
      }
    },
    "/api/Analyzes/{id}/Finalize": {
      "post": {
        "tags": [
          "Analyzes"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FinalizeAnalysisModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/FinalizeAnalysisModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/FinalizeAnalysisModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Analyzes/{id}/Class": {
      "post": {
        "tags": [
          "Analyzes"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ClassAnalysisModel"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ClassAnalysisModel"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ClassAnalysisModel"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Token": {
      "post": {
        "tags": [
          "Token"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "grant_type": {
                    "type": "string"
                  },
                  "client_id": {
                    "type": "string"
                  },
                  "client_secret": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "grant_type": {
                  "style": "form"
                },
                "client_id": {
                  "style": "form"
                },
                "client_secret": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/TokenModelResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenModelResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenModelResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/TokenErrorModelResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AnalysisControlFlowModel": {
        "type": "object",
        "properties": {
          "projectKey": {
            "type": "string",
            "nullable": true
          },
          "testsFail": {
            "type": "integer",
            "format": "int32"
          },
          "testsPass": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "AnalysisControlFlowResponseModel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          }
        },
        "additionalProperties": false
      },
      "ClassAnalysisModel": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "nullable": true
          },
          "code": {
            "type": "string",
            "format": "byte",
            "nullable": true
          },
          "lines": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/LineAnalysisModel"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "FinalizeAnalysisModel": {
        "type": "object",
        "properties": {
          "messageError": {
            "type": "string",
            "description": "Message error is required if status is Error.",
            "nullable": true
          },
          "status": {
            "$ref": "#/components/schemas/StatusFinalize"
          }
        },
        "additionalProperties": false
      },
      "LineAnalysisModel": {
        "type": "object",
        "properties": {
          "method": {
            "type": "string",
            "nullable": true
          },
          "numberLine": {
            "type": "integer",
            "format": "int32"
          },
          "cef": {
            "type": "integer",
            "format": "int32"
          },
          "cep": {
            "type": "integer",
            "format": "int32"
          },
          "cnf": {
            "type": "integer",
            "format": "int32"
          },
          "cnp": {
            "type": "integer",
            "format": "int32"
          },
          "suspiciousValue": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "StatusFinalize": {
        "enum": [
          0,
          1
        ],
        "type": "integer",
        "description": "Define 0 to Error and 1 to Success.",
        "format": "int32"
      },
      "TokenErrorModelResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "nullable": true
          },
          "error_description": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TokenModelResponse": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string",
            "nullable": true
          },
          "token_type": {
            "type": "string",
            "nullable": true
          },
          "expires_in": {
            "type": "integer",
            "format": "int32"
          },
          "username": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}